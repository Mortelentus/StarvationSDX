<configs>
<config name="blocks">	
<append xpath="/blocks">
	<block id="1493" name="SolarPannel">
		<property name="IsDeveloper" value="true" />
		<property name="Class" value="Loot" />
		<property name="Material" value="metal" />
		<property name="Shape" value="ModelEntity" />
		<property name="Model" value="#sunenergy?SolarCollector" />
		<property name="FuelValue" value="100" />
		<property name="LootList" value="6" />
		<drop event="Destroy" name="scrapIron" count="1" />		
	</block>
	<block id="1494" name="WeaponWorkbench">
		<property name="Class" value="AssWork, Mods" />
		<property name="Material" value="metal" />
		<property name="StabilitySupport" value="false" />
		<property name="Shape" value="ModelEntity" />
		<property name="Model" value="#weaponworkbench?WeaponWorkbenchPrefabV" />
		<property name="MultiBlockDim" value="2,2,1" />
		<property name="Place" value="TowardsPlacerInverted" />
		<property name="Stacknumber" value="1" />
		<property name="HeatMapStrength" value="0.5" />
		<property name="HeatMapTime" value="1200" />
		<property name="HeatMapFrequency" value="25" />
		<!--<property name="RecipeList" value="backpack, workbench"/>-->
		<property name="RecipeUnlock" value="backpack, workbench"/>
		<property class="Workstation">
			<property name="Modules" value="output" />
			<property name="CraftingAreaRecipes" value="player,workbench" />
		</property>
		<property class="RepairItems">
			<property name="forgedIron" value="35" />
			<property name="wood" value="50" />
		</property>
		<drop event="Harvest" name="" count="0" tool_category="Disassemble" />
		<drop event="Destroy" count="0" />
		<drop event="Destroy" name="WeaponWorkbench" count="1" tool_category="Disassemble" />
		<property name="Group" value="Building" />
		<property name="EconomicValue" value="776" />
		<property name="Place" value="TowardsPlacerInverted" />
		<property name="RotationAllowed" value="pivotOnFloor" />
	</block>
	<block id="1495" name="WeaponWorkbench1">
		<property name="IsDeveloper" value="true" />
		<property name="Class" value="AssWork, Mods" />
		<property name="Material" value="metal" />
		<property name="StabilitySupport" value="false" />
		<property name="Shape" value="ModelEntity" />
		<property name="Model" value="#weaponworkbench?WeaponWorkbenchPrefabV" />
		<property name="MultiBlockDim" value="2,2,1" />
		<property name="Place" value="TowardsPlacerInverted" />
		<property name="Stacknumber" value="1" />
		<property name="HeatMapStrength" value="0.5" />
		<property name="HeatMapTime" value="1200" />
		<property name="HeatMapFrequency" value="25" />
		<property name="RecipeUnlock" value="backpack, workbench"/>
		<property class="Workstation">
			<property name="Modules" value="output" />
			<property name="CraftingAreaRecipes" value="player,workbench" />
		</property>
		<property class="RepairItems">
			<property name="forgedIron" value="35" />
			<property name="wood" value="50" />
		</property>
		<drop event="Harvest" name="" count="0" tool_category="Disassemble" />
		<drop event="Destroy" count="0" />
		<drop event="Destroy" name="WeaponWorkbench1" count="1" tool_category="Disassemble" />
		<property name="Group" value="Building" />
		<property name="EconomicValue" value="776" />
		<property name="Place" value="TowardsPlacerInverted" />
		<property name="RotationAllowed" value="pivotOnFloor" />
	</block>	
	<block id="1496" name="BatteryPack">
		<property name="IsDeveloper" value="true" />
		<property name="Class" value="Loot" />
		<property name="Material" value="metal" />
		<property name="Shape" value="ModelEntity" />
		<property name="Model" value="#batteries?BatteryBlock" />
		<property name="FuelValue" value="100" />
		<property name="LootList" value="6" />
		<drop event="Destroy" name="scrapIron" count="1" />		
	</block>	
</append>
</config>
<config name="xui">	
<append xpath="/xui/ruleset[@name='default']">
	<window_group name="workstation_WeaponWorkbench" controller="XUiC_WorkstationWindowGroup">		
		<window name="windowCraftingList" />
		<window name="windowToolsForge" />
		<window name="craftingInfoPanel" />
		<window name="windowCraftingQueue" />			
		<window name="windowOutput" />
		<window name="windowNonPagingHeader" />
	</window_group>	
	<window_group name="workstation_WeaponWorkbench1" controller="XUiC_WorkstationWindowGroup">		
		<window name="windowCraftingList" />
		<window name="windowToolsForge" />
		<window name="craftingInfoPanel" />
		<window name="windowCraftingQueue" />			
		<window name="windowOutput" />
		<window name="windowNonPagingHeader" />
	</window_group>
</append>
</config>
