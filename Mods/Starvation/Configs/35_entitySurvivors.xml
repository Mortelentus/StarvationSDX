<config name="quests">
<append xpath="/quests">
	<quest id="quest_nuclearPlant1" group_name="Nuclear Plant" name="Nuclear Plant" subtitle="Nuclear Plant" description="Well....\n\nSince you've asked so nicely, I have marked your map\nwith the nearest Nuclear Plant location.\n\nI advise you to approach with care.\n\nIf you find it, come back to me and we'll talk more..." icon="ui_game_symbol_map_trader" category_key="Quest">
		<objective type="Goto" id="PowerPlant" value="5" />
		<reward type="SkillPoints" value="1" />
	</quest>
	<quest id="quest_nuclearPlant2" group_name="Nuclear Plant" name="Power it UP" subtitle="Power it UP" description="Good job locating that thing!\n\nNow if only you could power it up and get me some plutonium,\ni'm sure I could teach you something interesting..." icon="ui_game_symbol_map_trader" category_key="Quest">
		<objective type="FetchKeep" id="plutonium" value="1" />
		<reward type="Item" id="radbook" />
	</quest>
	<quest id="quest_repairKatana1" group_name_key="quest_repairKatana" name="Repair My Gun 1/2" name_key="quest_repairKatana1" subtitle="Get me some stuff" description="You seem resourceful. If you could get me some leather and\nforged iron to repair my Katana, maybe we could talk...\n\nGet this stuff and come back to me!" icon="ui_game_symbol_hammer" category_key="Quest">
		<action type="TrackQuest" />
		<objective type="FetchKeep" id="leather" value="5" />
		<objective type="FetchKeep" id="forgedSteel" value="2" />
		<reward type="SkillPoints" value="1" />
	</quest>
	<quest id="quest_repairKatana2" name="quest_repairKatana" subtitle="Need some more help" description="This mats will certainly help me,\nbut I'm not getting out of here with so many zeds around.\n\nDo some cleaning, and I may follow you!\nI really HATE female zombies, their screams are disgusting!\n\nCome back to me when you're done!" icon="ui_game_symbol_zombie" category_key="challenge">
		<objective type="ZombieKill" id="zombieArlene" value="1" />
		<objective type="ZombieKill" id="zombieDarlene" value="1" />
		<objective type="ZombieKill" id="zombieMarlene" value="1" />
		<objective type="ZombieKill" id="zombieNurse" value="1" />
		<reward type="SkillPoints" value="1" />
	</quest>
		<quest id="quest_repairKatana3" name="quest_bringMeMoreFlesh" subtitle="Get me some more...." description="There are still to many of those suckers running around,\nbring me a few more and I got a nice present for you" icon="ui_game_symbol_zombie" category_key="challenge">		
		<objective type="ZombieKill" id="zombieBoe" value="1" />
		<objective type="ZombieKill" id="zombieJoe" value="1" />
		<objective type="ZombieKill" id="zombieMoe" value="1" />
		<objective type="ZombieKill" id="zombieYo" value="1" />
		<reward type="Item" id="talkingHeadItem" />
		<reward type="SkillPoints" value="1" />		
	</quest>
	<quest id="quest_militaryAid" name="quest_militaryAid" name_key="quest_militaryAid" subtitle="I want revenge!" description="My entire unit was killed inside the radiated zone!\nI want you to kill that damn mutant bosses for me!\n\nI'll make it worth while!" icon="ui_game_symbol_zombie" category_key="challenge">
		<objective type="ZombieKill" id="RadBoss" value="1" />
		<objective type="ZombieKill" id="RadBossOld" value="1" />
		<reward type="Item" id="aviatorGoggles" />
	</quest>
	<quest id="quest_militaryReward" name="quest_militaryAid" subtitle="I want revenge!" description="AH AH AH AH! You got them fuckers!\n\nI hope you brought me their goggles as prove!\n\nWear them for me, please! This is awsome!!" icon="ui_game_symbol_zombie" category_key="Quest">
		<objective type="Wear" id="aviatorGoggles" />
		<reward type="Item" id="MilitaryScrewer" />
	</quest>
		<quest id="quest_TheMGJunkyard" group_name="The Old Mg" name="Old MG Junkyard" subtitle="Find the Old MG" description="a way to the Junkyard with the old MG" icon="ui_game_symbol_map_trader" category_key="Quest">
	  <objective type="Goto" id="junkyard_med_MG" value="5" />
	  <reward type="SkillPoints" value="1" />
	 </quest>
	<quest id="quest_TheFarmer" group_name="The Farmer" name="The Farmer" subtitle="Find the Farmer" description="Where the hell is the farmer" icon="ui_game_symbol_map_trader" category_key="Quest">
	  <objective type="Goto" id="FarmerPrefab" value="5" />
	  <reward type="SkillPoints" value="1" />
	 </quest>
	 <quest id="quest_TheScientists" group_name="The Scientists" name="The Scientists" subtitle="Find the Scientists" description="Scientists, where are they hiding" icon="ui_game_symbol_map_trader" category_key="Quest">
	  <objective type="Goto" id="ScientistShelter" value="5" />
	  <reward type="SkillPoints" value="1" />
	 </quest>
	<quest id="quest_Michone" group_name="The Katana Lady" name="The Katana Lady" subtitle="Find Michone" description="Find Michones House" icon="ui_game_symbol_map_trader" category_key="Quest">
	  <objective type="Goto" id="abandoned_house_michone" value="5" />
	  <reward type="SkillPoints" value="1" />
	 </quest>
	<quest id="quest_Madmole" group_name="Tha Church" name="Holy Rooms" subtitle="Find Madmole" description="Find Madmoles Church" icon="ui_game_symbol_map_trader" category_key="Quest">
	  <objective type="Goto" id="church_madmole" value="5" />
	  <reward type="SkillPoints" value="1" />
	</quest>	 
	<quest id="quest_Military" group_name="Military" name="Get da Beast" subtitle="Find Trooper" description="Find Trooper" icon="ui_game_symbol_map_trader" category_key="Quest">
		<objective type="Goto" id="MilitaryBarracks" value="5" />
		<reward type="SkillPoints" value="1" />
	</quest>
	<quest id="quest_PrepareForTheCure" group_name="The Cure" name="Is there a Cure?" subtitle="Get your Rad Suit to help us" description="You know what, take this key and get your Hazmat Suit.\n\nYou'll probably need it, if you're gonna help us.\n\nTalk to me when you have it!" icon="ui_game_symbol_map_trader" category_key="Quest">
		<reward type="Item" id="RadSuitKey" />
		<reward type="Quest" id="quest_theCure" />
	</quest>
	<quest id="quest_theCure" group_name="The Cure" name="Is there a Cure? 1" subtitle="Wear the Hazmat" description="Wear the full hazmat suit, and talk with the doc again." icon="ui_game_symbol_map_trader" category_key="Quest">
		<objective type="FetchKeep" id="hazmatMask" value="1" />
		<objective type="FetchKeep" id="hazmatGloves" value="1" />
		<objective type="FetchKeep" id="hazmatShirt" value="1" />
		<objective type="FetchKeep" id="hazmatPants" value="1" />
		<objective type="FetchKeep" id="hazmatBoots" value="1" />
		<reward type="SkillPoints" value="1" />
	</quest>
	<quest id="quest_theCure1" group_name="The Cure" name="Is there a Cure? 2" subtitle="Find the infected Scientist" description="We've recently sent a team to the outer radiated zones.\nWe suspected there may be something there that can help us...\nUnfortunately we lost contact with our team,\nand we fear for the worse.\nMy brother was one of the team members,\nand I was wondering if you could go look for him there.\n\nAnd... ermmm... If he's infected, please make sure you free him...\n\nOh and if he's carying any syringe,\nmake sure you bring it back with you!" icon="ui_game_symbol_map_trader" category_key="Quest">
		<objective type="ZombieKill" id="DocZd" value="1" />
		<objective type="FetchKeep" id="RadSyringe" value="1" />
		<reward type="Item" id="antibiotics" />
	</quest>
	<quest id="quest_theCure2" group_name="The Cure" name="Is there a Cure? 3" subtitle="One step closer" description="Well, shit... That's unfortunate about my brother.\n\nHowever the syringe you brought back\nis VERY interesting! This cures almost any disease...\nCareful though, it's quite addictive!\n\nHere, you keep that one and let me teach you\nhow to make more!\n\nBye now, i'll keep investigating a definite cure!!" icon="ui_game_symbol_map_trader" category_key="Quest">
		<objective type="FetchKeep" id="RadSyringe" value="1" />
		<reward type="Item" id="radSyringeBook" />
	</quest>
	<quest id="quest_farmer1" group_name="Save my Ass" name="Save my Ass" subtitle="Seed Me" description="Hi there pilgrim!\n\nI wonder if you could help me with something!\nI've been trying to restart my farm, but I need some seeds..." icon="ui_game_symbol_map_trader" category_key="Quest">
		<action type="TrackQuest" />
		<objective type="FetchKeep" id="potatoSeed" value="3" />
		<objective type="FetchKeep" id="cornSeed" value="2" />
		<objective type="FetchKeep" id="wheat" value="1" />
		<reward type="SkillPoints" value="1" />
	</quest>
	<quest id="quest_farmer2" group_name="Save my Ass" name="Save my Ass" subtitle="Save My Ass" description="Perfect... But this damn zeds keep destroying my plants!\n\nI'm just not strong enough... Could you help me?\nYou do this for me, and I'll get you a nice ass!\n\nAre you ready for this?" icon="ui_game_symbol_map_trader" category_key="Quest">
		<action type="TrackQuest" />
		<objective type="ZombieKill" id="zombieBoe" value="1" />
		<objective type="ZombieKill" id="zombieJoe" value="1" />
		<reward type="Item" id="donkeyItem" />
	</quest>
	<quest id="quest_BurnedHouse" group_name="Test" name="BurnedHouse" subtitle="BurnedHouse" description="..." icon="ui_game_symbol_map_trader" category_key="Quest">
		<objective type="Goto" id="HouseBurntRadiated" value="5" />
		<reward type="SkillPoints" value="1" />
	</quest>
	<quest id="quest_BurnedHouse1" group_name="Test" name="BurnedHouse1" subtitle="BurnedHouse1" description="..." icon="ui_game_symbol_map_trader" category_key="Quest">
		<objective type="Goto" id="HouseBurntRadiated1" value="5" />
		<reward type="SkillPoints" value="1" />
	</quest>	
	<quest id="quest_MpWaste" group_name="Test" name="MpWaste" subtitle="MpWaste" description="..." icon="ui_game_symbol_map_trader" category_key="Quest">
		<objective type="Goto" id="MpWasteRadiated" value="5" />
		<reward type="SkillPoints" value="1" />
	</quest>	
</append>	  
</config>
