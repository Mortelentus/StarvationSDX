		<property name="TimeStayAfterDeath" value="300" />
		<!-- Set this if you want this entity to be spawned when user chooses 'enemy entities off' -->
		<property name="IsEnemyEntity" value="false" />
		<property name="ExperienceGain" value="150" />
		<property name="CorpseBlock" value="animalGore" />
		<property name="CorpseBlockChance" value="1" />
		<property name="DeadBodyHitPoints" value="150" />
		<property name="IsMale" value="false" />
		<!-- FOOD CONFIGURATION - <amount of "hp" to restore>,foodItem1,foodItem2,...,foodItemn -->
		<!-- Lower type of food - survivor looks for this if he is not hurt -->
		<property name="FoodLow" value="1,salad,wheat,carrot" />
		<!-- Medium type of food - survivor looks for this if he is slighlty hurt -->
		<property name="FoodMedium" value="2,carrot" />
		<!-- Higher type of food - survivor looks for this if he is badly hurt -->
		<property name="FoodHigh" value="5,apple" />
		<!-- Food containers where survivors look for something to eat - NEEDS to be secureLoot -->
		<property name="FoodContainer" value="cntStorageFood" />
		<!-- Food Check interval in seconds -->
		<property name="CheckInterval" value="200" />
		<!-- Food check area in blocks -->
		<property name="CheckArea" value="10" />
		<!-- Cannot stack because it can be picked up -->
		<property name="Stacknumber" value="1" />
		<property name="debug" value="false" />
		<!-- to allow interaction and/or put stuff on its inv? -->
		<property name="LootListAlive" value="10" />
		<property name="LootListAliveMP" value="10" />
		<!-- For dialogs -->
		<!-- (<xxx>) means optional -->
		<!-- param1="<skillName>,<skillLevel>" -->
		<!-- param2="<Command>,(<questName>),(<previousQuestName>)" -->
		<property name="Dialog1" value="Here boy, follow!" param1="Husbandry,5" param2 = "Follow"/>
		<property name="Dialog2" value="Stay and defend!" param1="Husbandry,5" param2 = "Patrol"/>
		<property name="Dialog3" value="Let me see what you have" param1="Husbandry, 10" param2 = "Inv"/>
		<!-- Plays random sounds -->
		<property name="RandomSounds" value="false" />
		<!-- sleeps -->
		<property name="DoSleep" value="false" />
		<!-- For harvesting -->
		<drop event="Harvest" name="rawMeat" count="3" />
		<drop event="Harvest" name="rawMeat" tool_category="Butcher" count="1" />
		<drop event="Harvest" name="rawMeat" tool_category="Butcher" count="1" prob=".5" />
		<drop event="Harvest" name="rawMeat" tool_category="Butcher" count="1" prob=".5" />
		<drop event="Harvest" name="animalHide" tool_category="Butcher" count="7" />
		<drop event="Harvest" name="animalHide" tool_category="Butcher" count="4" prob=".5" />
		<drop event="Harvest" name="animalFat" tool_category="Butcher" count="1" prob=".4" />
		<drop event="Harvest" name="animalFat" tool_category="Butcher" count="4" />
		<!-- Average harvest results:  stag  5 meat  9 hide  4.5 fat,  pig  6m  7h  6f,  bear 7m  14h  7f -->
	</entity_class>
</append>
</config>
