<configs>
<config name="items">
<append xpath="/items">
<item id="" name="handGirl">
	<property name="Extends" value="handMaster" />
	<property name="IsDeveloper" value="true" />
	<property class="Action0"> <!-- AttackAction -->
		<property name="DamageEntity" value="5" />
		<property name="DamageBlock" value="2" />		
	</property>
</item>
</append>	  
</config>
<config name="entityclasses">
<append xpath="/entity_classes"> 
	<entity_class name="survivorParent">
		<!-- Mesh and class properties -->
		<property name="Mesh" value="#Girl1?TeenGirl" />
		<property name="ModelType" value="Standard" />
		<property name="Prefab" value="NPC" />
		<property name="Class" value="EntitySurvivorMod, Mods" />
		<property name="Parent" value="Animals" />
		<property name="AvatarController" value="GameObjectSurvivorAnimationSDX, Mods" />
		<property name="Weight" value="70" />
		<property name="RotateToGround" value="true" />
		<property name="IsEntityAnimal" value="true" />
		<property name="PhysicsBody" value="Stag" />
		<property name="HandItem" value="handGirl" />
				<!-- AI properties -->
		<property name="AITask-1" value="Swim" />
		<property name="AITask-2" value="RunawayWhenHurt" />
		<property name="AITask-3" value="Territorial" param1="10" />
		<!-- <property name="AITask-4" value="RunawayFromEntity" param1="EntityZombie" /> -->
		<property name="AITask-4" value="Wander" />
		<!-- give up time -->		
		<property name="AITask-5" value="ApproachSpot" />		
		<property name="AITarget-1" value="SetAsTargetIfHurt" />
		<property name="AITarget-2" value="BlockingTargetTask" />		
		<property name="CanClimbLadders" value="true" />
		<!-- General entity properties -->
		<property name="MaxHealth" value="130" />
		<property name="MaxViewAngle" value="180" />
		<property name="WanderSpeed" value="0.25" />
		<property name="PanicSpeed" value="1.5" />
		<property name="ApproachSpeed" value="1.3" />
		<property name="NightWanderSpeed" value="0.25" />
		<property name="NightApproachSpeed" value="1.4" />			
		<!-- This decides on the particle effect played on hit -->
		<property name="SurfaceCategory" value="organic" />
		<property name="ParticleOnDeath" value="blood_death" />
		<property name="SoundHurt" value="Player_Female/player2painlg" />
		<property name="SoundHurtSmall" value="Player_Female/player2painsm" />
		<property name="SoundDeath" value="Player_Female/player2death" />
		<property name="SoundStamina" value="Player_Female/player2stamina" />
		<property name="SoundDrownPain" value="Player_Female/player2drownpain" />
		<property name="SoundWaterSurface" value="Player_Female/player2drownsurface" />
		<property name="SoundDrownDeath" value="Player_Female/player2drowndeath" />
		<property name="SoundJump" value="Player_Male/player2Jump" />
		<property name="SoundLandSoft" value="Player_Male/player2LandSoft" />
		<property name="SoundLandHard" value="Player_Male/player2LandHard" />
		<property name="SoundPlayerLandThump" value="PlayerLandThump" />
		<property name="TimeStayAfterDeath" value="300" />
		<!-- Set this if you want this entity to be spawned when user chooses 'enemy entities off' -->
		<property name="IsEnemyEntity" value="false" />
		<property name="ExperienceGain" value="150" />
		<property name="CorpseBlock" value="animalGore" />
		<property name="CorpseBlockChance" value="1" />
		<property name="DeadBodyHitPoints" value="150" />
		<property name="IsMale" value="false" />
		<!-- FOOD CONFIGURATION - <amount of "hp" to restore>,foodItem1,foodItem2,...,foodItemn -->
		<!-- Lower type of food - survivor looks for this if he is not hurt -->
		<property name="FoodLow" value="1,cornBread,eggboiled" />
		<!-- Medium type of food - survivor looks for this if he is slighlty hurt -->
		<property name="FoodMedium" value="2,grilledMeat,boiledMeat" />
		<!-- Higher type of food - survivor looks for this if he is badly hurt -->
		<property name="FoodHigh" value="5,meatStew,vegetableStew" />
		<!-- Food containers where survivors look for something to eat - NEEDS to be secureLoot -->
		<property name="FoodContainer" value="cntStorageFood" />
		<!-- Food Check interval in seconds -->
		<property name="CheckInterval" value="200" />
		<!-- Food check area in blocks -->
		<property name="CheckArea" value="10" />
		<!-- Cannot stack because it can be picked up -->
		<property name="Stacknumber" value="1" />
		<property name="debug" value="false" />
		<!-- to allow interaction and/or put stuff on its inv? -->
		<property name="LootListAlive" value="102" />
		<property name="LootListAliveMP" value="102" />
		<!-- Plays random sounds -->
		<property name="RandomSounds" value="true" />
		<!-- sleeps -->
		<property name="DoSleep" value="true" />
		<!-- For harvesting -->
		<drop event="Harvest" name="rawMeat" count="3" />
		<drop event="Harvest" name="rawMeat" tool_category="Butcher" count="1" />
		<drop event="Harvest" name="rawMeat" tool_category="Butcher" count="1" prob=".5" />
		<drop event="Harvest" name="rawMeat" tool_category="Butcher" count="1" prob=".5" />
		<drop event="Harvest" name="animalHide" tool_category="Butcher" count="7" />
		<drop event="Harvest" name="animalHide" tool_category="Butcher" count="4" prob=".5" />
		<drop event="Harvest" name="animalFat" tool_category="Butcher" count="1" prob=".4" />
		<drop event="Harvest" name="animalFat" tool_category="Butcher" count="4" />
		<!-- Average harvest results:  stag  5 meat  9 hide  4.5 fat,  pig  6m  7h  6f,  bear 7m  14h  7f -->
	</entity_class>
	
