		</entity_class>
		<entity_class name="animalBabyCow" extends="animalCow">
			<!-- Mesh and class properties
			<property name="Mesh" value="Animals/Stag/STAG" />
			<property name="ModelType" value="Standard" />-->
			<property name="Prefab" value="NPC" />
			<property name="Mesh" value="#Animals?Cow" />
			<property name="ModelType" value="Standard" />
			<property name="AvatarController" value="GameObjectAnimalAnimationSDX, Mods" />
			<property name="MeshScale" value="0.5,0.5" /> <!-- on SP it will slowly grow, but on MP it will go from this size to mature size -->
