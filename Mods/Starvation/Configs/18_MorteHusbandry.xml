			<property name="isBaby" value="true" />
			<!-- For harvesting -->
			<drop event="Harvest" name="rawMeat" count="1" />
			<drop event="Destroy" name="femur" tool_category="Butcher" count="1" />
			<property name="MeshScale" value="0.4,0.3" />
		</entity_class>
