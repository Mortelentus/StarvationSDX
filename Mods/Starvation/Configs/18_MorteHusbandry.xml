<config name="entitygroups">
	<!-- removing vanilla pigs -->
	<remove xpath="/entitygroups/entitygroup[@name='AnimalsAll']/entity[@name='animalPig']" />
	<remove xpath="/entitygroups/entitygroup[@name='AnimalsFarm']/entity[@name='animalPig']" />
	<remove xpath="/entitygroups/entitygroup[@name='ZombieFarmGroup']/entity[@name='animalPig']" />
	
	<append xpath="/entitygroups/entitygroup[@name='AnimalsAll']">
		<entity name="animalFemalePig" prob="0.01" />
		<entity name="animalMalePig" prob="0.005" />
		<entity name="animalCow" prob="0.01" />
		<entity name="animalBull" prob="0.0051" />
		<entity name="animalSheep" prob="0.01" />
		<entity name="animalRam" prob="0.005" />
	</append>
	<append xpath="/entitygroups/entitygroup[@name='AnimalsFarm']">
		<entity name="animalFemalePig" prob="0.01" />
		<entity name="animalMalePig" prob="0.005" />
		<entity name="animalSheep" prob="0.01" />
		<entity name="animalRam" prob="0.005" />
	</append>
	<append xpath="/entitygroups/entitygroup[@name='ZombieFarmGroup']">
		<entity name="animalFemalePig" prob="0.01" />
		<entity name="animalMalePig" prob="0.005" />
	</append>	
</config>

<config name="items">
	<append xpath="/items">
		<!-- HUSBANDRY -->
		<item id="" name="cowItem">
			<property name="Meshfile" value="Animals/Stag/STAG" />
			<property name="MeshMaterial" value="#husbandry?Cow1" />
			<property name="Material" value="organic" />
			<property name="HoldType" value="7" />
			<property name="Stacknumber" value="1" />
			<property class="Action1">
				<property name="Class" value="SpawnMorteAnimal, Mods" />
				<property name="entityToSpawn" value="animalCow" />
			</property>
			<property class="Preview">
				<property name="Zoom" value="28" />
				<property name="Pos" value="0,-0.05" />
				<property name="Rot" value="90,45,0" />
			</property>
			<property name="EconomicValue" value="300" />
		</item>
		<item id="" name="bullItem">
			<property name="Extends" value="cowItem" />
			<property class="Action1">
				<property name="entityToSpawn" value="animalBull" />
			</property>
			<property name="EconomicValue" value="400" />
		</item>
		<item id="" name="sheepItem">
			<property name="Extends" value="cowItem" />
			<property name="MeshMaterial" value="#husbandry?sheep1" />
			<property class="Action1">
				<property name="entityToSpawn" value="animalSheep" />
			</property>
			<property name="EconomicValue" value="300" />
		</item>
		<item id="" name="ramItem">
			<property name="Extends" value="sheepItem" />
			<property class="Action1">
				<property name="entityToSpawn" value="animalRam" />
			</property>
			<property name="EconomicValue" value="400" />
		</item>
		<item id="" name="pigFemaleItem">
			<property name="Extends" value="cowItem" />
