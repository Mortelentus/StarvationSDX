			</property>
			<property name="EconomicValue" value="300" />
		</item>
		<item id="" name="pigMaleItem">
			<property name="Extends" value="pigFemaleItem" />
			<property class="Action1">
				<property name="entityToSpawn" value="animalMalePig" />
			</property>
			<property name="EconomicValue" value="400" />
		</item>

		<item id="" name="whip">
