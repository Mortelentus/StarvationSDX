<configs>
<config name="recipes">
	<append xpath="/recipes">
	<!-- HUSBANDRY -->
	<recipe name="whip" count="1" scrapable="False">
		<ingredient name="leather" count="2"/>
		<ingredient name="wood" count="15"/>
	</recipe>
	<recipe name="bucketWood" count="1" scrapable="False">
		<ingredient name="wood" count="20"/>
		<ingredient name="forgedIron" count="5"/>
	</recipe>
	<recipe name="trough" count="1" scrapable="False">
		<ingredient name="wood" count="50"/>
		<ingredient name="forgedIron" count="10"/>
	</recipe>
	<recipe name="scissor" count="1" scrapable="False">
		<ingredient name="forgedIron" count="2"/>
		<ingredient name="ductTape" count="3"/>
	</recipe>
	<!-- HUSBANDRY -->
	</append>	  
</config>

<config name="entityclasses">
	<append xpath="/entity_classes">
		<!-- HUSBANDRY -->		
		<!-- Wandertime IN SECONDS is how much time the animal will idle wandering, after an action -->
		<!-- Matingtime IN TICKS is the interval between mating tries for the males -->
		<!-- PregnancyTime IN TICKS is the time a female will be pregnant after a successful mating -->
		<!-- Adolesence IN TICKS is how much time an offspring will take to mature -->
		<!-- 1 day = 24000 ticks (for 60min day) -->
		<entity_class name="animalMalePig">
			<!-- Mesh and class properties -->
			<property name="Mesh" value="Animals/Pig/PIG" />
			<property name="ModelType" value="Standard" />
			<property name="Prefab" value="NPC" />
			<property name="MeshMaterial" value="#husbandry?pig2"/>
			<property name="MeshScale" value="1,1" />
			<property name="Parent" value="Animals" />
			<property name="AvatarController" value="GameObjectAnimalAnimation" />
			<property name="Weight" value="70" />
			<property name="RotateToGround" value="true" />
			<property name="CorpseBlock" value="animalGore" />
			<property name="CorpseBlockChance" value="1" />
			<property name="IsEntityAnimal" value="true" />
			<property name="PhysicsBody" value="Stag" />
			<property name="Class" value="EntityAnimalTameMorte, Mods" />
			<!-- AI properties -->				
			<property name="AITask-1" value="Wander" />
			<property name="AITask-2" value="RunawayWhenHurt" />
			<property name="AITask-4" value="RunawayFromEntity" param1="EntityZombie" />
			<property name="AITask-4" value="RunawayFromEntity" param1="EntityAnimalBear" />
			<property name="AITask-5" value="Swim" />
			<!-- <property name="AITask-5" value="RunawayFromEntity" param1="EntityPlayer" />/ -->
			<property name="CanClimbLadders" value="false" />
			<!-- General entity properties -->
			<property name="MaxHealth" value="50" />
			<property name="MaxViewAngle" value="180" />
			<property name="WanderSpeed" value="0.3" />
			<property name="PanicSpeed" value="1.0" />
			<!-- This decides on the particle effect played on hit -->
			<property name="SurfaceCategory" value="organic" />
			<property name="ParticleOnDeath" value="blood_death" />
			<property name="SoundHurt" value="Animals/pigpain" />
			<property name="SoundDeath" value="Animals/pigdeath" />
			<!-- custom properties -->
			<property name="isMale" value="true" />
			<property name="entityMale" value="animalMalePig" />
			<property name="entityFemale" value="animalFemalePig" /> 
			<property name="entityBaby" value="animalBabyPig" />
			<property name="itemTamed" value="pigMaleItem" /> 
			<property name="foodBlock" value="trough" /> <!-- the block they look for when hungry -->
			<property name="foodItem" value="hayBaleBlock" /> <!-- the item they eat -->
			<property name="harvestItem" value="bucketWood" /> <!-- the item used to harvest the animal -->
			<property name="tameItem" value="whip" /> <!-- the item used to tame the animal -->
			<property name="lootItem" value="turd" /> <!-- the item harvested -->
			<property name="tameSkill" value="Husbandry" /> <!-- the skill needed to tame -->
			<property name="harvestSkill" value="Husbandry" /> <!-- the skill needed to tame -->
			<!-- 24000 = 1 in-game day -->
			<property name="Adolesence" value="24000" />
			<property name="PregnancyTime" value="24000" />
			<property name="WanderTime" value="180" />
			<property name="MatingTime" value="2400" />
			<property name="MaxAnimalsInArea" value="15" />
			<property name="MaxAnimalsAreaCheck" value="40" />
			<property name="MaxMatingAreaCheck" value="10" />
			<!-- custom properties -->
			<property name="TimeStayAfterDeath" value="180" />
			<!-- Set this if you want this entity to be spawned when user chooses 'enemy entities off' -->
			<property name="IsEnemyEntity" value="false" />
			<property name="ExperienceGain" value="150" />
			<!-- For harvesting -->
			<property name="DeadBodyHitPoints" value="200" />
			<property name="PanicSpeed" value="1.2" />
			<property name="PhysicsBody" value="pig" />
			<property name="ExperienceGain" value="150" />
			<!-- For harvesting -->
			<drop event="Harvest" name="rawMeat" count="2" />
			<drop event="Harvest" name="rawMeat" tool_category="Butcher" count="1" prob=".5" />
			<drop event="Harvest" name="rawMeat" tool_category="Butcher" count="1" prob=".5" />
			<drop event="Harvest" name="animalHide" tool_category="Butcher" count="3" />
			<drop event="Harvest" name="animalHide" tool_category="Butcher" count="1" prob=".5" />
			<drop event="Harvest" name="animalHide" tool_category="Butcher" count="1" prob=".5" />
			<drop event="Harvest" name="animalFat" tool_category="Butcher" count="2" />
			<drop event="Harvest" name="animalFat" tool_category="Butcher" count="1" prob=".5" />
			<drop event="Harvest" name="animalFat" tool_category="Butcher" count="1" prob=".5" />
			<drop event="Harvest" name="femur" tool_category="Butcher" count="1" prob=".4" />
			<drop event="Harvest" name="femur" tool_category="Butcher" count="1" prob=".4" />
			<drop event="Destroy" name="femur" tool_category="Butcher" count="2" />
			<property name="MeshScale" value="1.3,1.3" />
		</entity_class>
		<entity_class name="animalFemalePig" extends="animalMalePig">
			<property name="Mesh" value="Animals/Pig/PIG" />
			<property name="MeshMaterial" value="#pig2?pig2"/>
			<!-- custom properties -->
			<property name="isMale" value="false" />
			<property name="itemTamed" value="pigFemaleItem" />
			<!-- For harvesting -->
			<drop event="Harvest" name="rawMeat" count="2" />
			<drop event="Harvest" name="rawMeat" tool_category="Butcher" count="1" prob=".5" />
			<drop event="Harvest" name="rawMeat" tool_category="Butcher" count="1" prob=".5" />
			<drop event="Harvest" name="animalHide" tool_category="Butcher" count="3" />
			<drop event="Harvest" name="animalHide" tool_category="Butcher" count="1" prob=".5" />
			<drop event="Harvest" name="animalHide" tool_category="Butcher" count="1" prob=".5" />
			<drop event="Harvest" name="animalFat" tool_category="Butcher" count="2" />
			<drop event="Harvest" name="animalFat" tool_category="Butcher" count="1" prob=".5" />
			<drop event="Harvest" name="animalFat" tool_category="Butcher" count="1" prob=".5" />
			<drop event="Harvest" name="pigFemaleItem" tool_category="Butcher" count="1" prob=".005" />
			<drop event="Harvest" name="pigMaleItem" tool_category="Butcher" count="1" prob=".001" />
			<drop event="Destroy" name="femur" tool_category="Butcher" count="2" />
			<property name="MeshScale" value="0.8,0.7" />
		</entity_class>
		<entity_class name="animalBabyPig" extends="animalMalePig">
			<property name="Mesh" value="Animals/Pig/PIG" />
			<property name="MeshMaterial" value="#pig2?pig2"/>
