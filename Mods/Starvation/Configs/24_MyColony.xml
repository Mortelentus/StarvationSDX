		<!-- Requirement: Male/Female/Teen/Any survivor type -->
		<property name="Requires" value="Male" />
		<!-- CRAFTING CONFIGURATION -->
		<!-- Used to look for the receipts the crafter will use. If it cannot find on the set section, it will look for one with no craftarea -->
		<!-- Omit or leave blank if you want to use the first receipt for the products -->		
		<property name="CraftArea" value="smith" />
		<!-- Lower tier of product - low skill required -->
		<property name="LowTier" value="trapNail,ceramicPlate,hoeIron,wrench,partsRevolver_frame,partsRevolver_grip,partsPumpShotgun_stock,partsPumpShotgun_barrel" />
		<!-- Medium tier of product - some skill required (6 or higher)  -->
		<property name="MedTier" value="huntingKnife,trapBear,forgedSteel,fireaxeIron,pickaxeIron,partsHuntingRifle_stock,partsHuntingRifle_barrel,partsPistol_grip,partsPistol_barrel,partsPumpShotgun_stockShort,partsPumpShotgun_barrelShort" />
		<!-- High tier of product - highly skilled (12 or higher)-->
		<property name="HighTier" value="machete,garageDoorRolling,steelPlate,armorPlate,partsAK47_stock,partsAK47_barrel,partsMP5_stock,partsMP5_barrel,metalFrame,smallDeposit" />
		<!-- Expert product - Crafter needs to be maxed out (15) -->
		<property name="ExpertTier" value="powerStreetLight01,fireaxeSteel,assaultRifleStock,assaultRifleBarrel,partsSniperRifle_stock,partsSniperRifle_barrel,genCasing,fuelDeposit" />
		<!-- Receipts to autolearn -->
		<property name="AutoLearn" value="metalFrame,smallDeposit,genCasing,fuelDeposit" />
		<!-- Maximum of tier Default recipes the crafter can automatically know -->
		<property name="LowTierDF" value="3" />
		<property name="MedTierDF" value="3" />
		<property name="HighTierDF" value="2" />
		<property name="ExpertTierDF" value="2" />
		<!-- Raw Materials Container - NEEDS to be secureLoot -->
		<property name="MatContainer" value="cntStorageBuilding" />
