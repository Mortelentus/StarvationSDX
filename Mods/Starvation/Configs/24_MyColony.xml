		<drop event="Destroy" count="0" prob="1" />
		<property name="CanMobsSpawnOn" value="false" />
		<!-- CUSTOM HEATMAP - emited while working -->
		<!-- this values are similar to vanilla config -->
		<property name="HeatStrength" value="3" />
