<config name="items">	
<append xpath="/items">	
	<item id="" name="survivorMale">		
		<property name="DescriptionKey" value="survivorMaleDesc" />
		<property name="Meshfile" value="Items/Misc/sackPrefab" />
		<property name="DropMeshfile" value="Items/Misc/sack_droppedPrefab" />
		<property name="Weight" value="1" />
		<property name="Material" value="organic" />
		<property name="HoldType" value="45" />
		<property name="Stacknumber" value="1" />
		<!-- STK resource -->
		<property name="FuelValue" value="1" />
		<property class="Action1">
			<property name="Class" value="PlaceAsBlockM,Mods" />
			<property name="Delay" value="0.5" />
			<property name="Blockname" value="survivor" />
		</property>
		<property name="Group" value="Resources" />
	</item>
	<item id="" name="survivorFemale">
		<property name="Extends" value="survivorMale" />
		<property name="DescriptionKey" value="survivorFemaleDesc" />
	</item>
	<item id="" name="survivorTeen">
		<property name="Extends" value="survivorMale" />
		<property name="DescriptionKey" value="survivorTeenDesc" />		
	</item>	
</append>	
</config>
<!--			Blocks			-->
<config name="blocks">	
<append xpath="/blocks">
	<!-- MyColony -->
	<block id="1132" name="survivor">
		<property name="DescriptionKey" value="survivorDesc" />
		<property name="Class" value="Survivor, Mods" />
		<property name="Material" value="organic" />
		<property name="Shape" value="ModelEntity" />
		<property name="Model" value="#Survivors?survivorPrefab" />		
		<drop event="Destroy" count="0" prob="1" />
		<property name="CanMobsSpawnOn" value="false" />
		<property name="CanPickup" value="true" />
		<!-- FOOD CONFIGURATION - <amount of "hp" to restore>,foodItem1,foodItem2,...,foodItemn -->
		<!-- Lower type of food - survivor looks for this if he is not hurt -->
		<property name="FoodLow" value="1,cornBread,eggboiled" />
		<!-- Medium type of food - survivor looks for this if he is slighlty hurt -->
		<property name="FoodMedium" value="2,cornBread,grilledMeat,boiledMeat" />
		<!-- Higher type of food - survivor looks for this if he is badly hurt -->
		<property name="FoodHigh" value="5,meatStew,vegetableStew" />
		<!-- Food containers where survivors look for something to eat - NEEDS to be secureLoot -->
		<property name="FoodContainer" value="cntStorageFood" />
		<!-- Food Check interval in seconds -->
		<property name="CheckInterval" value="200" />
		<!-- Food check area in blocks -->
		<property name="CheckArea" value="10" />
		<!-- Cannot stack because it can be picked up -->
		<property name="Stacknumber" value="1" />
		<property name="debug" value="false" />
	</block>
	<block id="1133" name="RadioTower">
		<property name="DescriptionKey" value="RadioTowerDesc" />
		<property name="Class" value="RadioTower, Mods" />
		<property name="Material" value="Mbridge_metal" />
		<property name="Shape" value="ModelEntity" />
		<property name="Model" value="#Survivors?radioTowerPrefab" />		
		<drop event="Destroy" count="0" prob="1" />
		<property name="CanMobsSpawnOn" value="false" />
		<property name="CanPickup" value="true" />
		<!-- If you set this value to true, you WILL NEED TO USE POWER MOD -->
		<!-- If true it will require electrical power to work -->
		<property name="isPowered" value="true" />
		<!-- Spawn Chance in %, checked at each spawn interval -->
		<property name="SpawnChance" value="25" />
		<!-- Spawn interval in seconds -->
		<property name="SpawnInterval" value="120" />
		<!-- Spawn area in blocks -->
		<property name="SpawnArea" value="10" />
		<!-- EntityGroup to choose spawn -->
		<property name="EntityGroup" value="SurvivorsTower" />
		<!-- Max number of survivors allowed inside the spawn area -->
		<!-- Will include advanced survivors like cooks or traders -->
		<property name="MaxSpawn" value="5" />
		<property name="debug" value="false" />
	</block>
